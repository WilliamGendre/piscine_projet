{% extends 'publicView/base.html.twig' %}

{% block title %}Catégories{% endblock %}

{% block content %}

    <main>
        <h2 id="categoryTitle">Catégories</h2>
        <section id="categoryList">
            {% for category in categorys %}
                <h3><a href="{{ path('one_category', {'id': category.id}) }}">{{ category.libelle }}</a></h3>
            {% endfor %}
        </section>
    </main>

{% endblock %}